#include <stdio.h>
int main (void)
{
	long first_number, second_number, ans ;
	printf("Enter the first number : \n ");
	scanf("%ld",&first_number);
	printf("Enter the secone number: \n ");
	scanf("%ld", &second_number);
	ans = first_number * second_number ;
	printf("%ld * %ld = %ld \n", first_number, second_number, ans);
	return (0);
}

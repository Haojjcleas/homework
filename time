#include <stdio.h>
int main (void)
{
	double first_number = 0, second_number = 1;
	int ans = 0 ,a ,b;
	printf("Enter the first number : \n ");
	scanf("%lf",&first_number);
	printf("Enter the secone number: \n ");
	scanf("%lf", &second_number);
	ans = (int) first_number * (int) second_number ;
	a = (int) first_number;
	b = (int) second_number;
	printf("%d * %d = %d \n", a, b, ans);
	return (0);
}
